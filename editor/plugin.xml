<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated cv -->
      <category
            id="org.nasdanika"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.nasdanika.cv.presentation.cvModelWizardID"
            name="%_UI_cvModelWizard_label"
            class="org.nasdanika.cv.presentation.CvModelWizard"
            category="org.nasdanika"
