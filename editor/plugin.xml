      <editor
            id="org.nasdanika.cv.presentation.cvEditorID"
            name="%_UI_cvEditor_label"
            icon="icons/full/obj16/cvModelFile.gif"
            extensions="nsdcv"
            class="org.nasdanika.cv.presentation.CvEditor"
            contributorClass="org.nasdanika.cv.presentation.CvActionBarContributor">
      </editor>
